<h1>Artists#show</h1>
<p><%= @artist.first_name %> <%= @artist.last_name %></p>

<p>
  <%= link_to "Back to Artists", artists_path %> |
  <%= link_to "New", new_artist_path(@artist) %> |
  <%= link_to "Edit", edit_artist_path(@artist) %> |
  <%= link_to "Delete", @artist, method: :delete, data: {confirm: "Are you sure?"} %>
</p>
