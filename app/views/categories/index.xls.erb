<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="Number">ID</Data></Cell>
        <Cell><Data ss:Type="String">Category Name</Data></Cell>
        <Cell><Data ss:Type="String">Art Type</Data></Cell>
        <Cell><Data ss:Type="String">Category </Data></Cell>
        <Cell><Data ss:Type="Number">Sort Order</Data></Cell>
      </Row>
    <% @value_items.each do |value_item| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= category.id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= category.name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= category.art_type %></Data></Cell>
        <Cell><Data ss:Type="String"><%= category.category %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= category.sort %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
