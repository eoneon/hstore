<p id="notice"><%= notice %></p>

<h1>Listing Dimension Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dimension_types.each do |dimension_type| %>
      <tr>
        <td><%= dimension_type.name %></td>
        <td><%= link_to 'Show', dimension_type %></td>
        <td><%= link_to 'Edit', edit_dimension_type_path(dimension_type) %></td>
        <td><%= link_to 'Destroy', dimension_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dimension type', new_dimension_type_path %>
